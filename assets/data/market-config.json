{
  "version": "2.0.0",
  "environment": "production",
  "api": {
    "baseUrl": "https://api.rayzw2e.com/v1",
    "timeout": 30000,
    "retries": 3,
    "rateLimit": {
      "requests": 1000,
      "window": 60000
    }
  },
  "markets": {
    "crypto": {
      "enabled": true,
      "updateInterval": 1000,
      "exchanges": [
        {
          "name": "binance",
          "url": "https://api.binance.com",
          "fees": {
            "maker": 0.001,
            "taker": 0.001
          }
        },
        {
          "name": "coinbase",
          "url": "https://api.exchange.coinbase.com",
          "fees": {
            "maker": 0.005,
            "taker": 0.005
          }
        },
        {
          "name": "kraken",
          "url": "https://api.kraken.com",
          "fees": {
            "maker": 0.002,
            "taker": 0.002
          }
        }
      ],
      "symbols": [
        {
          "symbol": "BTCUSDT",
          "base": "BTC",
          "quote": "USDT",
          "pricePrecision": 2,
          "quantityPrecision": 8,
          "minQuantity": 0.00001,
          "maxQuantity": 1000,
          "minPrice": 0.01,
          "maxPrice": 1000000
        },
        {
          "symbol": "ETHUSDT",
          "base": "ETH",
          "quote": "USDT",
          "pricePrecision": 2,
          "quantityPrecision": 8,
          "minQuantity": 0.001,
          "maxQuantity": 10000,
          "minPrice": 0.01,
          "maxPrice": 100000
        },
        {
          "symbol": "BNBUSDT",
          "base": "BNB",
          "quote": "USDT",
          "pricePrecision": 2,
          "quantityPrecision": 8,
          "minQuantity": 0.01,
          "maxQuantity": 10000,
          "minPrice": 0.01,
          "maxPrice": 10000
        }
      ]
    },
    "forex": {
      "enabled": true,
      "updateInterval": 2000,
      "providers": [
        {
          "name": "fxcm",
          "url": "https://api.fxcm.com",
          "spread": 0.1
        },
        {
          "name": "oanda",
          "url": "https://api.oanda.com",
          "spread": 0.2
        }
      ],
      "pairs": [
        {
          "symbol": "EURUSD",
          "base": "EUR",
          "quote": "USD",
          "pipPrecision": 4,
          "minQuantity": 0.01,
          "maxQuantity": 1000000,
          "spread": 0.1
        },
        {
          "symbol": "GBPUSD",
          "base": "GBP",
          "quote": "USD",
          "pipPrecision": 4,
          "minQuantity": 0.01,
          "maxQuantity": 1000000,
          "spread": 0.2
        },
        {
          "symbol": "USDJPY",
          "base": "USD",
          "quote": "JPY",
          "pipPrecision": 2,
          "minQuantity": 0.01,
          "maxQuantity": 1000000,
          "spread": 0.1
        }
      ]
    },
    "stocks": {
      "enabled": true,
      "updateInterval": 3000,
      "exchanges": [
        {
          "name": "NYSE",
          "url": "https://api.nyse.com",
          "timezone": "America/New_York"
        },
        {
          "name": "NASDAQ",
          "url": "https://api.nasdaq.com",
          "timezone": "America/New_York"
        }
      ],
      "symbols": [
        {
          "symbol": "AAPL",
          "name": "Apple Inc.",
          "exchange": "NASDAQ",
          "sector": "Technology",
          "pricePrecision": 2,
          "minQuantity": 1,
          "maxQuantity": 10000
        },
        {
          "symbol": "GOOGL",
          "name": "Alphabet Inc.",
          "exchange": "NASDAQ",
          "sector": "Technology",
          "pricePrecision": 2,
          "minQuantity": 1,
          "maxQuantity": 10000
        },
        {
          "symbol": "MSFT",
          "name": "Microsoft Corp.",
          "exchange": "NASDAQ",
          "sector": "Technology",
          "pricePrecision": 2,
          "minQuantity": 1,
          "maxQuantity": 10000
        }
      ]
    },
    "commodities": {
      "enabled": true,
      "updateInterval": 5000,
      "types": [
        {
          "name": "precious_metals",
          "symbols": [
            {
              "symbol": "XAUUSD",
              "name": "Gold Spot",
              "unit": "oz",
              "pricePrecision": 2,
              "minQuantity": 0.01,
              "maxQuantity": 1000
            },
            {
              "symbol": "XAGUSD",
              "name": "Silver Spot",
              "unit": "oz",
              "pricePrecision": 3,
              "minQuantity": 0.01,
              "maxQuantity": 10000
            }
          ]
        },
        {
          "name": "energy",
          "symbols": [
            {
              "symbol": "XTIUSD",
              "name": "Crude Oil",
              "unit": "barrel",
              "pricePrecision": 2,
              "minQuantity": 1,
              "maxQuantity": 10000
            },
            {
              "symbol": "XNGUSD",
              "name": "Natural Gas",
              "unit": "MMBtu",
              "pricePrecision": 3,
              "minQuantity": 1,
              "maxQuantity": 10000
            }
          ]
        }
      ]
    },
    "bonds": {
      "enabled": true,
      "updateInterval": 10000,
      "types": [
        {
          "name": "government",
          "symbols": [
            {
              "symbol": "US10Y",
              "name": "10 Year Treasury",
              "country": "US",
              "pricePrecision": 3,
              "minQuantity": 1000,
              "maxQuantity": 1000000
            }
          ]
        },
        {
          "name": "corporate",
          "symbols": [
            {
              "symbol": "AAPL2030",
              "name": "Apple 2030 Bond",
              "issuer": "Apple Inc.",
              "pricePrecision": 3,
              "minQuantity": 1000,
              "maxQuantity": 1000000
            }
          ]
        }
      ]
    }
  },
  "ai": {
    "enabled": true,
    "models": {
      "neural": {
        "type": "deep_learning",
        "parameters": 50000000,
        "accuracy": 0.94,
        "updateInterval": 5000
      },
      "quantum": {
        "type": "quantum_optimization",
        "qubits": 1024,
        "speedup": 1000,
        "updateInterval": 10000
      },
      "predictive": {
        "type": "time_series",
        "horizon": 72,
        "confidence": 0.89,
        "updateInterval": 15000
      },
      "sentiment": {
        "type": "nlp",
        "sources": 1000000,
        "languages": ["en", "zh", "ja", "es", "fr"],
        "updateInterval": 30000
      }
    },
    "confidence": 0.8,
    "maxPositions": 50,
    "riskLevel": "moderate"
  },
  "security": {
    "encryption": {
      "algorithm": "AES-256",
      "keySize": 256
    },
    "biometric": {
      "enabled": true,
      "types": ["fingerprint", "face", "voice"]
    },
    "twoFactor": {
      "enabled": true,
      "methods": ["totp", "sms", "email"]
    },
    "sessionTimeout": 3600000,
    "maxLoginAttempts": 5,
    "lockoutDuration": 900000
  },
  "performance": {
    "maxConcurrentRequests": 100,
    "cacheSize": 1000,
    "compression": true,
    "cdn": {
      "enabled": true,
      "provider": "cloudflare"
    },
    "monitoring": {
      "enabled": true,
      "metrics": ["response_time", "throughput", "error_rate"],
      "alertThreshold": {
        "response_time": 1000,
        "error_rate": 0.01
      }
    }
  },
  "notifications": {
    "email": {
      "enabled": true,
      "provider": "sendgrid",
      "from": "noreply@rayzw2e.com"
    },
    "sms": {
      "enabled": true,
      "provider": "twilio"
    },
    "push": {
      "enabled": true,
      "provider": "fcm"
    },
    "webhook": {
      "enabled": true,
      "timeout": 5000
    }
  },
  "features": {
    "autoTrading": {
      "enabled": true,
      "maxPositions": 50,
      "maxLeverage": 100,
      "minConfidence": 0.7
    },
    "backtesting": {
      "enabled": true,
      "maxDataPoints": 100000,
      "maxStrategies": 100
    },
    "analytics": {
      "enabled": true,
      "retention": 365,
      "granularity": ["1m", "5m", "15m", "1h", "1d"]
    },
    "api": {
      "enabled": true,
      "rateLimit": {
        "requests": 1000,
        "window": 60000
      },
      "keyManagement": {
        "type": "jwt",
        "expiry": 3600
      }
    }
  }
}
